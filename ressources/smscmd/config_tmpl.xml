<config>
    <version>1</version>
    <!-- Serial -->
    <serial_device>#device#</serial_device>
    <pin>#pin#</pin>
    <text_mode>#text_mode#</text_mode>
    <smsc>#smsc#</smsc>
    <serial_rate>#serial_rate#</serial_rate>
    <!-- Trigger -->
    <trigger_url>#trigger_url#</trigger_url>
    <apikey>#apikey#</apikey>
    <trigger_timeout>10</trigger_timeout>
    <!-- Daemon -->
    <daemon_pidfile>#pid_path#</daemon_pidfile>
    <!-- Logging -->
    <loglevel>error</loglevel>
    <logfile>#log_path#.error</logfile>
    <!-- Socket server -->
    <sockethost>#sockethost#</sockethost>
    <socketport>#socketport#</socketport>
</config>
